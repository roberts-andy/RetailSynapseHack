					"%%sql \r\n",
					"\r\n",
					"create view vProcessedData\r\n",
					"as\r\n",
					"select year(to_date(h.Date)), month(to_date(h.Date)), count(*)\r\n",
					"  from default.transactiondetails d\r\n",
					"  join default.transactionheader h\r\n",
					"    on d.TransactionHeaderID = h.TransactionHeaderID\r\n",
					"  join default.store s\r\n",
					"    on h.StoreRetailID = s.StoreRetailID\r\n",
					"  group by year(to_date(h.Date)), month(to_date(h.Date))\r\n",
					"limit 10\r\n",
					"\r\n",
